{
  "name": "ishiro-mobile",
  "version": "0.1.0",
  "description": "ðŸ“± Ishiro mobile app for iOS and Android",
  "license": "GPL-3.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "tsc",
    "test": "jest",
    "test:pre-commit": "jest -o",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:pre-commit"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "pretty-quick --staged",
      "yarn lint"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@expo-google-fonts/poppins": "^0.1.0",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "dotenv": "^8.2.0",
    "expo": "~39.0.2",
    "expo-application": "^2.3.0",
    "expo-auth-session": "^2.0.0",
    "expo-status-bar": "~1.0.2",
    "formik": "^2.2.5",
    "graphql": "^15.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
    "react-native-confirmation-code-field": "^6.5.1",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "^12.1.0",
    "react-native-web": "~0.13.12",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/named-operations-object": "^1.17.9",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.1",
    "@graphql-codegen/typescript-operations": "1.17.10",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.4.1",
    "@testing-library/react-native": "^7.0.2",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/yup": "^0.29.9",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "import-sort-style-module-alias": "^1.1.0",
    "jest-expo": "^39.0.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.2.0",
    "prettier-plugin-import-sort": "^0.0.6",
    "pretty-quick": "^3.0.0",
    "typescript": "~3.9.5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module-alias",
      "parser": "typescript",
      "options": {
        "alias": [
          "app",
          "assets",
          "components",
          "routes",
          "shared"
        ]
      }
    }
  }
}
